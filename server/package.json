{
  "private": true,
  "name": "@conheca-meta/server",
  "version": "0.0.0",
  "description": "Desafio meta api",
  "main": "src/server/server.js",
  "scripts": {
    "start": "cross-env node src/server/server.js",
    "dev": "cross-env nodemon --exec \"yarn start\"",
    "start-dev": "cross-env NODE_ENV=development && cross-env node server/server.js",
    "start-dev-watch": "cross-env nodemon --exec \"yarn run start-dev\"",
    "test": "cross-env NODE_ENV=test && cross-env mocha ./src/**/*.test.js",
    "test-watch": "cross-env nodemon --exec \"yarn run test\"",
    "coverage": "cross-env nyc --check-coverage --lines 80 --per-file --all --reporter html --reporter text yarn test",
    "dev-coverage": "cross-env nyc --all --reporter html --reporter text yarn test",
    "postinstall": "cross-env link-module-alias",
    "preinstall": "cross-env link-module-alias && cross-env link-module-alias clean || true",
    "jsdoc": "cross-env jsdoc -c jsdoc.json",
    "swagger-doc": "cross-env node_modules/swagger-jsdoc/bin/swagger-jsdoc.js -d swaggerDef.js",
    "docs": "cross-env yarn jsdoc && cross-env yarn swagger-doc",
    "lint": "eslint --cache ./src --ext .js"
  },
  "_moduleAliases": {
    "~root": ".",
    "~app": "src/application",
    "~dom": "src/domain",
    "~inf": "src/infraestructure",
    "~inf@logger": "src/infraestructure/logger",
    "~inf@salesforce-conn": "src/infraestructure/salesforce",
    "~dom@models": "src/domain/models"
  },
  "repository": {
    "type": "git",
    "url": "http://gitlab.meta.com.br/tvglobo/microservice-nodejs-chassis.git"
  },
  "keywords": [
    "node",
    "nodejs",
    "microservice",
    "chassis",
    "api",
    "rest",
    "endpoint",
    "service"
  ],
  "author": "Giuseppe M Pereira",
  "license": "MIT",
  "dependencies": {
    "@azure/service-bus": "^1.0.3",
    "JSONStream": "^1.3.5",
    "axios": "^0.18.0",
    "bluebird": "^3.5.5",
    "body-parser": "^1.19.0",
    "cls-hooked": "^4.2.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "csv": "^5.1.1",
    "csv-parse": "^4.4.3",
    "csv-split-stream": "^1.0.2",
    "csv-stringify": "^5.3.0",
    "dotenv": "^7.0.0",
    "etl": "^0.6.5",
    "express": "^4.16.4",
    "express-pino-logger": "^4.0.0",
    "express-request-id": "^1.4.1",
    "express-statsd": "^0.3.0",
    "helmet": "^3.16.0",
    "highland": "^2.13.5",
    "jsforce": "^1.9.2",
    "kafka-node": "^4.1.3",
    "knex": "^0.16.5",
    "moment": "^2.24.0",
    "multer": "^1.4.1",
    "mysql": "^2.17.1",
    "nock": "^10.0.6",
    "node-schedule": "^1.3.2",
    "oracledb": "^3.1.2",
    "passport": "^0.4.0",
    "pg": "^7.9.0",
    "pino": "^5.12.6",
    "pino-pretty": "^3.2.0",
    "sequelize": "^5.2.11",
    "ship-hold": "^2.0.1",
    "simple-banner": "^1.0.7",
    "stream-transform": "^2.0.1",
    "swagger-jsdoc": "^3.4.0",
    "swagger-ui-express": "^4.0.7",
    "tap": "^13.1.3",
    "tcomb": "^3.2.29"
  },
  "devDependencies": {
    "better-docs": "^1.1.6",
    "chai": "^4.2.0",
    "chai-json": "^1.0.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^5.0.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "expect": "^24.7.0",
    "js-yaml": "^3.13.0",
    "jsdoc": "^3.6.3",
    "link-module-alias": "^1.2.0",
    "mocha": "^6.1.4",
    "nodemon": "^1.19.1",
    "prettier": "1.18.2",
    "request": "^2.88.0",
    "supertest": "^4.0.2",
    "typescript": "^3.5.2"
  }
}
